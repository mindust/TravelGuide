<script type="text/template" id="tpl-review">
	<div class="review-content"><div class="rating rating-stars s<%- parseFloat(rating) * 10 %>"><span><%- rating %></span>/<span>5</span> 星</div><div class="title"><strong><%- title %></strong></div><div class="byline"><span class="reviewed-by"> 评论人</span>&ensp;<span class="author"><% if (author) { %><%= author %>,<% } else { %>GetYourGuide 客户,<% } %></span>&ensp;<span class="date"><%- date %></span></div><div class="description"><p><%- message %></p></div></div></script>
<script>_.extend(gyg.dictionary, {'addingToCart': '正在加入购物车...','genericErrorMsg': '发生错误。','searchError': '糟糕，出错了！请稍后再试。','closeModalText': '确定','pleaseSelect': '请选择...','openingTimes': '营业/营运时间 :'});</script>
<script type="text/template" id="tpl-peoplepicker-input"><div class="label"><p class="picker-info<% if (ageLabel.length === 0) { %> no-age-info<%}%>"><span class="name"><%- name %></span> <span class="age"><%= ageLabel %></span> </p><div class="people-select-container"><button id="people-picker-minus" class="icon-minus"></button><input class="input-num peoplepicker-input" type="tel" pattern="[0-9]*" value="<%- value %>" min="0" max="69" step="1" maxlength="2" autocomplete="off" /><button id="people-picker-plus" class="icon-plus"></button></div></div></script>
<script type="text/template" id="tpl-peoplepicker-summary"><span class="name"><%- name %></span> x <span class="val"><%- value %></span><% if (seperator) { %>,  <% } %></script>
<script type="text/template" id="tpl-activity-option"><% if (isAvailable) { %><header class="header"><div class="hgroup"><h3 class="hed"><%- title %></h3><% if (deal) { %><h5 class="discount">节省高达 <%- deal_percentage %>%</h5><% }else if (future_deal) { %><h5 class="discount future-deal">特别优惠&nbsp;:&nbsp;<%- future_deal_start %>&nbsp;-&nbsp;<%- future_deal_end %></h5><% } %></div><div class="price-display"><div class="price-summary"><div class="price-tag"><span class="label total">总价</span><span class="price"><span class="currency"><%- currency_symbol %></span><span class="total-price"><%- total_price %></span></span><% if (deal) { %><span class="original-price"><span class="currency"><%- currency_symbol %></span><span class="original-total-price"><%- total_price_without_deal %></span></span><% } %></div><a href="#" class="show-price-details">价格细目</a></div><div class="price-details<% if (active_price_details) { %> is-active<% } %>"><div class="price-table"><% _.each(priceBreakdown, function(category) { %><div class="price-category"><span class="age-group"><%= category.title %>&nbsp;:</span><span class="price-calc"><%- category.total_participants %>&nbsp;x&nbsp;<%- category.price_per_person %></span><span class="price-category-total"><%- currency_symbol %>&nbsp;<%= category.total_price %></span></div><% }); %></div></div></div> <% if (description) { %><div class="description"><%= description %></div><% } %></header><div class="details"><div class="time-selection"><div class="hed">选择起始时间:</div><div class="times"></div></div><div class="add-to-cart"><button class="btn btn-booking btn-small">加入购物车</button></div><div class="msg"><div class="msg-content"></div></div></div><% } else { %><header class="header"><div class="hgroup"><h3 class="hed"><%- title %></h3></div><div class="msg">不可预订</div></header><div class="unavailable"><% if (next_availability) { %><div class="next-avail">下一个可预订日期: <span class="next-avail-date"><%- next_availability %></span></div><% } else { %><div class="msg"><%- reason %></div><% } %></div><% } %></script>
<script type="text/template" id="tpl-alternative-tours"><div id="recommended-tour-modal" class="modal fade multiple-options" role="dialog" tabindex="-1" aria-hidden="true"><div class="recommended-modal-container"><div class="recommended-tour" id="recommended-tour"><button class="recommended-modal-close icon icon-cancel"></button><h3 class="recommended-headline">不好意思，那一天超受欢迎呢！</h3><h6 class="recommended-headline-small">您所选的日子没有余额了。可是我们为您找来了可预订的类似活动。</h6><hr/><div data-role="activity"><div class="activities-container"><% for(var recommendedItem in recommendedItems) { %><article id="t<%- recommendedItems[recommendedItem].id %>"class="activity"><div class="activity-container"><div class="activity-description"><div class="activity-image"><img data-role="cover" src='<%= recommendedItems[recommendedItem].img_url %>'alt="<%- recommendedItems[recommendedItem].title %>"onerror="this.parentNode.parentNode.className+=' missing-image'"/></div><div class="activity-details"><h2 class="activity-title"><%- recommendedItems[recommendedItem].title %></h2><div class="activity-review-description"><% if (recommendedItems[recommendedItem].total_rating) { %><div class="activity-rating"><span class="rating"title="评分: 4 / 5"><span class="rating-stars s<%- recommendedItems[recommendedItem].rating %> rating-stars--<%- recommendedItems[recommendedItem].rating %>"></span><span class="rating-total">(<%- recommendedItems[recommendedItem].total_rating %> 评论)</span></span></div><% } %><p class="recommended-description"><%- recommendedItems[recommendedItem].description %></p></div><div class="activity-pricing"><p class="activity-price"><span class="activity-price-from">起价</span><span class="activity-price-currency"><%- recommendedItems[recommendedItem].currency %><%- recommendedItems[recommendedItem].price %></span></p><a class="activity-details-link" href="<%- recommendedItems[recommendedItem].url %>" target="_blank"><span class="activity-read-more-button">查看详情</span></a></div></div></div></div><hr /></article><% }; %></div><hr/><a href="#" class="select-another-date">请挑选其他日子</a></div></div></div></div></script>
<script type="text/template" id="tpl-activity-price"><div class="price-summary"><div class="price-tag"><span class="label total">总价</span><span class="price"><span class="currency"><%- currency_symbol %></span><span class="total-price"><%- total_price %></span></span><% if (deal) { %><span class="original-price"><span class="currency"><%- currency_symbol %></span><span class="original-total-price"><%- total_price_without_deal %></span></span><% } %></div><a href="#" class="show-price-details">价格细目</a></div><div class="price-details<% if (active_price_details) { %> is-active<% } %>"><div class="price-table"><% _.each(priceBreakdown, function(category) { %><div class="price-category"><span class="age-group"><%= category.title %>&nbsp;:</span><span class="price-calc"><%- category.total_participants %>&nbsp;x&nbsp;<%- category.price_per_person %></span><span class="price-category-total"><%- currency_symbol %>&nbsp;<%= category.total_price %></span></div><% }); %></div></div></script>
<script type="text/template" id="tpl-activity-option-time"><% if (type === 'radioInput') { %><label class="time-radio"><input type="radio" name="time-<%= optionId %>" value="<%= id %>" <%- checked %> /><span class="txt"><%= start_time %></span></label><% } else if (type === 'selectOption') { %><option value="<%= id %>" <%- selected %> ><%- start_time %></option><% } else if (type === 'openingTime') { %><li><%- start_time %> - <%- end_time %></li><% } %></script>
