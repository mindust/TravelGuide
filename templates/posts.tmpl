<header><a href="/">Home</a></header>
	{{range .posts}}
	<h3><a href="/{{ .ID }}">{{ .ID }} {{ .NAME }}</a></h3>
	<div>{{ .COUNTRY }}</div>
	{{end}}


    <h2>Create a Post</h2>
    <form action="/" method="POST">
      <input type="text" name="COUNTRY" placeholder="COUNTRY" /><br>
        <input type="text" name="PROVINCE" placeholder="PROVINCE" /><br>
        <input type="text" name="CITY" placeholder="CITY" /><br>
        <input type="text" name="COUNTY" placeholder="COUNTY" /><br>
        <input type="text" name="NAME" placeholder="NAME" /><br>
        <input type="text" name="LEVEL" placeholder="LEVEL" /><br>
        <textarea name="LABEL"></textarea><br>
        <input type="text" name="PRICE" placeholder="PRICE" /><br>
        <input type="text" name="STATUS" placeholder="STATUS" /><br>
      <input type="submit" value="submit"/>
    </form>
